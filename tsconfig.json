{
  "compilerOptions": {
    "target": "ESNext",
    "lib": ["ESNext"],
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noUncheckedIndexedAccess": true,
    "esModuleInterop": true,
    "module": "ESNext",
    "moduleResolution": "Node",
    "noEmit": true
  },
  "ts-node": {
    "compilerOptions": {
      "module": "CommonJS"
    }
  }
}
